package switchDemo;
//grade:not
public class Main {

	public static void main(String[] args) {
		char grade = 'D';
		
		switch(grade) {
		case 'A':
			System.out.println("Mükemmel:Geçtiniz");
			break;
		case 'B':
			System.out.println("Çok güzel:Geçiniz");
			break;
		case 'C':
			System.out.println("Fena değil:Geçtiniz");
			break;
		case 'D':
			System.out.println("Maalesef Kaldınız");
			break;
		}
		
		

	}

}
